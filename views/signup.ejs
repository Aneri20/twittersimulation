    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script> <script src="http://code.jquery.com/jquery-migrate-1.2.1.min.js"></script></head>
<body  ng-app="signup" ng-controller="signup">
<p style="text-align:center"><img src="images/white_logo.png" style="position: absolute; top: 0px;z-index: 0;" alt="twitter" height="46px" width="46px"></p>
<img src="images/strip.png" style="position: absolute;align:center;left: 0px;top: 0px;z-index: -1;" alt="nav-bar" height="50px" width="100%">

    <div class="text-box">
       <!-- <div class="field-input">-->
    <div class="signup-wrapper">
        <h1>Join Twitter today</h1>
    <input type="text" class="textbox-control" name="full_name" id="fullname" placeholder="Full Name" required maxlength="20" ng-model="full_name"></br></br>
    <input type="email" class="textbox-control" name="email" id="email" placeholder="Phone or Email" required ng-model="email"> <div class="alert alert-danger" ng-hide="registered_email">
            <strong>This email is already registered. Want to login or recover your password?</strong>
        </div></br></br>
    <input type="password" class="textbox-control"name="password" id="inputPassword" placeholder="Password" required ng-model="password"></br></br>
    <div class="signup-wrapper">
        <div class="alert alert-danger" ng-hide="unexpected_error">
            <strong style="color:red">Unexpected error,try again</strong>
        </div>


           <input type="submit" style="color:white" class="submit-button" value="Sign-up" style="position:relative;left:0px;" ng-click="submit();"></br></br>
           <a style="color:#1da1f2" href="/login">Already have an account?Log in</a>
            </div>
</div>
        </div>


</body>
